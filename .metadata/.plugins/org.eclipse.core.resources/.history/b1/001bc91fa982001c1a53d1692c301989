import java.applet.Applet;
import java.awt.Color;
import java.util.*;
import java.awt.Frame;
import java.awt.Toolkit;
public class Applet_Frame extends Applet {
     public void init()
     {
    	 Frame f=new Frame();
    	 f.setUndecorated(false);
    	 f.setTitle("My Frame ");
    	 //f.setBackground(Color.YELLOW);
    	 f.setBackground(new Color(55,255,255));
    	 f.setCursor(6);
    	 f.setResizable(true);
    	 f.setVisible(true);
    	 f.setCursor(6);
    	 f.setResizable(true);
    	 f.setIconImage(Toolkit.getDefaultToolkit().getImage("C:\\Users\\itsme\\OneDrive\\Documents\\Java_Advance\\Java Frames\\src"));
    	 //you have a Image.jpg
    	 f.setSize(500,500);
    	 f.setLocation(200, 300);
    	 
     }
}
